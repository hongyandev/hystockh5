{
  "name": "shop-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "cross-env NODE_ENV=localhost vue-cli-service serve --config vue.config.js",
    "serve": "cross-env NODE_ENV=development vue-cli-service serve --config vue.config.js",
    "build": "cross-env NODE_ENV=production  vue-cli-service build --config vue.config.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vant": "^1.6.28",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-plugin-import": "^1.8.0",
    "cross-env": "^6.0.3",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "url-loader": "^2.2.0",
    "vue-lazyload": "^1.2.6",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 7"
  ]
}
